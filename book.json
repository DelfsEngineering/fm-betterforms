{
    "plugins": [ "theme-api" , "search", "hints"],
    "pluginsConfig": {
        "lunr": {
            "ignoreSpecialCharacters": true
        },
        "theme-api": {
            "theme":"dark",
            "split": true,
            "languages": [
                    {
                      "lang": "js",          
                      "name": "JavaScript",
                      "default": true
                    },
                {
                    "lang":"json",
                    "Name": "JSON",
                    "default":true
                },
                {
                    "lang": "filemaker",
                    "name": "FileMaker"
                }
            ]
        }
    }
}
